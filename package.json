{
    "name": "HackPotsdam",
    "version": "1.0.0",
    "description": "Hack Potsdam website, made with Bootstrap Agency theme.",
    "homepage": "https://startbootstrap.com/template-overviews/agency",
    "license": "MIT",
    "main": "index.js",
    "repository": {
        "type:": "git",
        "url": "https://github.com/hackpotsdam/hackpotsdam2018"
    },
    "scripts": {
        "start": "run-script-os",
        "start:win32": "browser-sync start --server --files '**/*.css, **/*.html, **/*.js, !node_modules/**/*' --directory --port 7777",
        "start:darwin:linux": "browser-sync start --server --files '**/*.css, **/*.html, **/*.js, !node_modules/**/*' --directory --port 7777 --no-open",
        "deploy": "gh-pages -d ./"
    },
    "dependencies": {
        "bootstrap": "4.0.0",
        "font-awesome": "4.7.0",
        "jquery": "3.3.1",
        "jquery.easing": "^1.4.1",
        "popper.js": "^1.14.3"
    },
    "devDependencies": {
        "browser-sync": "2.27.10",
        "gh-pages": "^1.1.0",
        "gulp": "^3.9.1",
        "gulp-clean-css": "3.9.2",
        "gulp-header": "2.0.1",
        "gulp-rename": "^1.2.2",
        "gulp-sass": "^3.1.0",
        "gulp-uglify": "3.0.0",
        "run-script-os": "^1.0.3"
    }
}
